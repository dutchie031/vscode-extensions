{
  "name": "excalidraw-sync",
  "displayName": "excalidraw-sync",
  "description": "Syncing my excalidraw drawings ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "publisher": "dutchie031",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/dutchie031/vscode-extensions.git"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "excalidraw-sync.removeS3Target",
        "title": "Remove Target",
        "category": "Dutchie Excalidraw Sync",
        "enablement": "view == excalidrawSyncPanel"
      },
      {
        "command": "excalidraw-sync.editS3Target",
        "title": "Edit Target",
        "category": "Dutchie Excalidraw Sync",
        "enablement": "view == excalidrawSyncPanel"
      },
      {
        "command": "excalidraw-sync.addS3Folder",
        "title": "Create Folder",
        "category": "Dutchie Excalidraw Sync",
        "enablement": "view == excalidrawSyncPanel"
      },
      {
        "command": "excalidraw-sync.addExcalidrawFile",
        "title": "Create Excalidraw File",
        "category": "Dutchie Excalidraw Sync",
        "enablement": "view == excalidrawSyncPanel"
      },
      {
        "command": "excalidraw-sync.deleteS3Bucket",
        "title": "Delete S3 Bucket",
        "category": "Dutchie Excalidraw Sync",
        "enablement": "view == excalidrawSyncPanel"
      }, 
      {
        "command": "excalidraw-sync.createExcalidrawFile",
        "title": "Create Excalidraw File",
        "category": "Dutchie Excalidraw Sync",
        "enablement": "view == excalidrawSyncPanel"
        
      },
      {
        "command" : "excalidraw-sync.deleteFile",
        "title": "Delete",
        "category": "Dutchie Excalidraw Sync",
        "enablement": "view == excalidrawSyncPanel"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "excalidrawSyncSidebar",
          "title": "Dutchie Excalidraw Sync",
          "icon": "media/logo.svg"
        }
      ]
    },
    "views": {
      "excalidrawSyncSidebar": [
        {
          "id": "excalidrawSyncPanel",
          "name": "Dutchie Excalidraw Sync",
          "icon": "media/logo.svg"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "when": "view == excalidrawSyncPanel && viewItem == excalidrawsync_s3target",
          "command": "excalidraw-sync.removeS3Target",
          "group": "navigation"
        },
        {
          "when": "view == excalidrawSyncPanel && viewItem == excalidrawsync_s3target",
          "command": "excalidraw-sync.editS3Target",
          "group": "navigation"
        },
        {
          "when": "view == excalidrawSyncPanel && viewItem == excalidrawsync_s3filesection",
          "command": "excalidraw-sync.addS3Folder",
          "group": "navigation"
        },
        {
          "when": "view == excalidrawSyncPanel && viewItem == excalidrawsync_s3directory",
          "command": "excalidraw-sync.addS3Folder",
          "group": "navigation"
        },
        {
          "when": "view == excalidrawSyncPanel && viewItem == excalidrawsync_s3bucket",
          "command": "excalidraw-sync.deleteS3Bucket",
          "group": "navigation"
        },
        {
          "when": "view == excalidrawSyncPanel && viewItem == excalidrawsync_s3directory",
          "command": "excalidraw-sync.createExcalidrawFile",
          "group": "navigation"
        },
        {
          "when": "view == excalidrawSyncPanel && viewItem == excalidrawsync_s3file",
          "command": "excalidraw-sync.deleteFile",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.978.0",
    "compile": "^0.0.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.108.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  }
}
